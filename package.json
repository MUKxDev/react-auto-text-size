    "build": "tsc & tsc --project tsconfig.cjs.json",
    "watch": "tsc-watch --onSuccess \"yalc push\""
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "tsc-watch": "^5.0.3",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@3.2.1"
}
